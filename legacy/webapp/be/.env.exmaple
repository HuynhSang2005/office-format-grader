# ========================
# Google AI Configuration
# ========================

# Tên mô hình AI sử dụng (ví dụ: models/gemini-2.5-flash-lite)
GOOGLE_AI_MODEL="YOUR_AI_MODEL_HERE"

# Độ sáng tạo của AI (0 = chính xác, 1 = sáng tạo). Giá trị đề xuất: 0.7
GOOGLE_AI_TEMPERATURE=0.7

# Số lần thử lại khi gọi API thất bại. Giá trị mặc định: 3
GOOGLE_AI_MAX_RETRIES=3

# Thời gian chờ giữa các lần retry (tính bằng mili giây). Giá trị mặc định: 1000ms = 1s
GOOGLE_AI_RETRY_DELAY_MS=1000
